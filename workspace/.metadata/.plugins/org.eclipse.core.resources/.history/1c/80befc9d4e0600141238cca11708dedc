package com.davidmis.thesis.network;

public class Phone {
	private String name;
	private String IP;
	private String port;
	
	public Phone(String n, String i) {
		name = n;
		IP = i;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	public String getIP() {
		return IP;
	}
	
	public void setIP(String iP) {
		IP = iP;
	}
	
	public String getPort() {
		return port;
	}
	
	public void setPort(String port) {
		this.port = port;
	}
}
